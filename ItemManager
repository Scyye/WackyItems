        meta.setUnbreakable(true);
        meta.addEnchant(Enchantment.DIG_SPEED, 4, true);
        meta.addEnchant(Enchantment.LOOT_BONUS_BLOCKS, 4, true);
        meta.addItemFlags(ItemFlag.HIDE_UNBREAKABLE);
