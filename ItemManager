    public static void createffboots() {
        ItemStack item = new ItemStack(Material.LEATHER_BOOTS, 1);
        ItemMeta meta = item.getItemMeta();
        meta.setDisplayName("§4Super Boots");
        List<String> lore = new ArrayList<>();
        lore.add("§7Prevents you from taking fire, or fall damage");
        lore.add("§7They were given this power by the one and only, §4§lPower§7!!");
        lore.add("");
        lore.add("§4§lULTRA-RARE BOOTS");
        meta.setLore(lore);
        meta.setUnbreakable(true);
        meta.addEnchant(Enchantment.PROTECTION_FALL, 255, true);
        meta.addEnchant(Enchantment.PROTECTION_FIRE, 255, true);
        meta.addEnchant(Enchantment.PROTECTION_ENVIRONMENTAL, 255, true);
        meta.addItemFlags(ItemFlag.HIDE_UNBREAKABLE);
        meta.addItemFlags(ItemFlag.HIDE_ATTRIBUTES);
        meta.addItemFlags(ItemFlag.HIDE_ENCHANTS);
        item.setItemMeta(meta);
