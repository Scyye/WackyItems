        sr4.setIngredient('n', Material.IRON_SWORD);
        Bukkit.getServer().addRecipe(sr4);

        ShapedRecipe sr5 = new ShapedRecipe(NamespacedKey.minecraft("gsword5"), item);
        sr5.shape("NNN", "NnN", "NNN");
        sr5.setIngredient('N', Material.AZALEA_LEAVES);
        sr5.setIngredient('n', Material.IRON_SWORD);
        Bukkit.getServer().addRecipe(sr5);

        ShapedRecipe sr6 = new ShapedRecipe(NamespacedKey.minecraft("gsword6"), item);
        sr6.shape("NNN", "NnN", "NNN");
        sr6.setIngredient('N', Material.DARK_OAK_LEAVES);
        sr6.setIngredient('n', Material.IRON_SWORD);
        Bukkit.getServer().addRecipe(sr6);

        ShapedRecipe sr7 = new ShapedRecipe(NamespacedKey.minecraft("gsword7"), item);
        sr7.shape("NNN", "NnN", "NNN");
        sr7.setIngredient('N', Material.FLOWERING_AZALEA_LEAVES);
        sr7.setIngredient('n', Material.IRON_SWORD);
        Bukkit.getServer().addRecipe(sr7);
    }

}
