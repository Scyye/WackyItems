    public static void createhsword() {
        ItemStack item = new ItemStack(Material.GOLDEN_SWORD, 1);
        ItemMeta meta = item.getItemMeta();
        meta.setDisplayName("§1Healthy Sword");
        List<String> lore = new ArrayList<>();
        lore.add("§7Hold this sword, and you");
        lore.add("§7will gain more health!");
        lore.add("");
        lore.add("§1§lEPIC SWORD");
        meta.setLore(lore);
        meta.setUnbreakable(true);
        meta.addEnchant(Enchantment.LUCK, 1, true);
        meta.addItemFlags(ItemFlag.HIDE_ENCHANTS);
        meta.addItemFlags(ItemFlag.HIDE_ATTRIBUTES);
        meta.addItemFlags(ItemFlag.HIDE_UNBREAKABLE);
        AttributeModifier modifier = new AttributeModifier(UUID.randomUUID(), "GENERIC_MAX_HEALTH", 40.0, AttributeModifier.Operation.ADD_NUMBER);
        AttributeModifier modifier2 = new AttributeModifier(UUID.randomUUID(), "GENERIC_MAX_HEALTH", 40.0, AttributeModifier.Operation.ADD_NUMBER, EquipmentSlot.OFF_HAND);
        meta.addAttributeModifier(Attribute.GENERIC_MAX_HEALTH, modifier);
        meta.addAttributeModifier(Attribute.GENERIC_MAX_HEALTH, modifier2);
        item.setItemMeta(meta);
        hsword = item;

        ShapedRecipe sr = new ShapedRecipe(NamespacedKey.minecraft("hsword"), item);
        sr.shape("XXX", "XxX", "XXX");
        sr.setIngredient('X', Material.GOLDEN_APPLE);
        sr.setIngredient('x', Material.WOODEN_SWORD);
        Bukkit.getServer().addRecipe(sr);
    }

    public static void creatercp() {
        ItemStack item = new ItemStack(Material.GOLDEN_CHESTPLATE, 1);
        ItemMeta meta = item.getItemMeta();
        meta.setDisplayName("§1Reverse Card Chestplate");
        List<String> lore = new ArrayList<>();
        lore.add("§7Uses the power of the uno gods");
        lore.add("§7to reverse all damage dealt to you");
        lore.add("§7back onto the dealer!");
        lore.add("");
        lore.add("§1§lEPIC CHESTPLATE");
        meta.setLore(lore);
        meta.setUnbreakable(true);
        meta.addEnchant(Enchantment.THORNS, 13, true);
        meta.addItemFlags(ItemFlag.HIDE_ENCHANTS);
        meta.addItemFlags(ItemFlag.HIDE_ATTRIBUTES);
        meta.addItemFlags(ItemFlag.HIDE_UNBREAKABLE);
        AttributeModifier modifier = new AttributeModifier(UUID.randomUUID(), "GENERIC_ARMOR", 8, AttributeModifier.Operation.ADD_NUMBER, EquipmentSlot.HAND);
        meta.addAttributeModifier(Attribute.GENERIC_ARMOR, modifier);
        item.setItemMeta(meta);
        rcp = item;

        ShapedRecipe sr = new ShapedRecipe(NamespacedKey.minecraft("rcp"), item);
        sr.shape("X X", "XxX", "cXc");
        sr.setIngredient('X', Material.NETHERITE_INGOT);
        sr.setIngredient('x', Material.NETHERITE_CHESTPLATE);
        sr.setIngredient('c', Material.CACTUS);
        Bukkit.getServer().addRecipe(sr);
    }

