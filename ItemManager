    public static void creategun() {
        ItemStack item = new ItemStack(Material.BOW);
        ItemMeta meta  = item.getItemMeta();
        meta.setDisplayName("§5Gun");
        List<String> lore = new ArrayList<>();
        lore.add("§7Commit Murder");
        lore.add("");
        lore.add("HAVE FUN");
        lore.add("");
        lore.add("§5§lSPECIAL GUN");
        meta.setLore(lore);
        meta.setUnbreakable(true);
        meta.addEnchant(Enchantment.ARROW_DAMAGE, 255, true);
        meta.addEnchant(Enchantment.ARROW_INFINITE, 1, true);
        meta.addEnchant(Enchantment.QUICK_CHARGE, 3, true);
        meta.addItemFlags(ItemFlag.HIDE_ENCHANTS);
        meta.addItemFlags(ItemFlag.HIDE_ATTRIBUTES);
        meta.addItemFlags(ItemFlag.HIDE_UNBREAKABLE);
        AttributeModifier ass = new AttributeModifier(UUID.randomUUID(), "GENERIC_ATTACK_SPEED", 255, AttributeModifier.Operation.MULTIPLY_SCALAR_1, EquipmentSlot.HAND);
        meta.addAttributeModifier(Attribute.GENERIC_ATTACK_SPEED, ass);
        item.setItemMeta(meta);
        gun = item;

        ShapedRecipe sr = new ShapedRecipe(NamespacedKey.minecraft("gun"), item);
        sr.shape("Nrr", " XX", "x  ");
        sr.setIngredient('X', Material.IRON_INGOT);
        sr.setIngredient('x', Material.STICK);
        sr.setIngredient('N', Material.GUNPOWDER);
        sr.setIngredient('r', Material.REDSTONE);
        Bukkit.getServer().addRecipe(sr);
    }

