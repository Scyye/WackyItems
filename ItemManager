package com.Scyye.WackyItems.items;

import org.bukkit.Bukkit;
import org.bukkit.Material;
import org.bukkit.NamespacedKey;
import org.bukkit.attribute.Attribute;
import org.bukkit.attribute.AttributeModifier;
import org.bukkit.enchantments.Enchantment;
import org.bukkit.inventory.EquipmentSlot;
import org.bukkit.inventory.ItemFlag;
import org.bukkit.inventory.ItemStack;
import org.bukkit.inventory.ShapedRecipe;
import org.bukkit.inventory.meta.ItemMeta;

import java.util.ArrayList;
import java.util.List;
import java.util.UUID;

public class ItemManager {


        // public static ItemStack section
    public static ItemStack pwrsword;
    public static ItemStack wmdpick;
    public static ItemStack wmdshovel;
    public static ItemStack wmdaxe;
    public static ItemStack ffboots;
    public static ItemStack ironpicke;
    public static ItemStack hdhelmet;
    public static ItemStack hdcp;
    public static ItemStack hdleg;
    public static ItemStack hdboots;
    public static ItemStack zsword;
    public static ItemStack hsword;
    public static ItemStack rcp;
    public static ItemStack kbstick;
    public static ItemStack gun;
    public static ItemStack iph;
    public static ItemStack gsword;


        // create methods
    public static void init() {
        createpwrsword();
        createwmdpick();
        createwmdshovel();
        createwmdaxe();
        createffboots();
        createironpicke();
        createhdhelmet();
        createhdcp();
        createhdleg();
        createhdboots();
        createzsword();
        createhsword();
        creatercp();
        createkbstick();
        creategun();
        createiph();
        creategsword();
    }

    public static void createpwrsword() {
        ItemStack item1 = new ItemStack(Material.WOODEN_SWORD, 1);
        ItemMeta meta = item1.getItemMeta();
        meta.setDisplayName("§4Power's Sword");
        List<String> lore = new ArrayList<>();
        lore.add("§7Yes, it belonged to 'Power'");
        lore.add("§7Uses the power of Power to do insane damage");
        lore.add("");
        lore.add("§4§lULTRA-RARE SWORD");
        meta.setLore(lore);
        meta.setUnbreakable(true);
        meta.addEnchant(Enchantment.DAMAGE_ALL, 1000, true);
        meta.addItemFlags(ItemFlag.HIDE_ENCHANTS);
        meta.addItemFlags(ItemFlag.HIDE_ATTRIBUTES);
        meta.addItemFlags(ItemFlag.HIDE_UNBREAKABLE);
        item1.setItemMeta(meta);
        pwrsword = item1;

        ShapedRecipe sr = new ShapedRecipe(NamespacedKey.minecraft("pwrsword"), item1);
        sr.shape("NHN", "HnH", "NHN");
        sr.setIngredient('N', Material.NETHERITE_BLOCK);
        sr.setIngredient('n', Material.NETHERITE_SWORD);
        sr.setIngredient('H', Material.HEART_OF_THE_SEA);
        Bukkit.getServer().addRecipe(sr);
    }

    public static void createwmdpick() {
        ItemStack item2 = new ItemStack(Material.WOODEN_PICKAXE, 1);
        ItemMeta meta = item2.getItemMeta();
        meta.setDisplayName("§4WMD Pick");
        List<String> lore = new ArrayList<>();
        lore.add("§7Power's power is so powerful, it can take the form");
        lore.add("§7of multiple different tools!");
        lore.add("");
        lore.add("§4§lULTRA-RARE PICKAXE");
        meta.setLore(lore);
        meta.setUnbreakable(true);
        meta.addEnchant(Enchantment.DIG_SPEED, 1000, true);
        meta.addItemFlags(ItemFlag.HIDE_ENCHANTS);
        meta.addItemFlags(ItemFlag.HIDE_UNBREAKABLE);
        meta.addItemFlags(ItemFlag.HIDE_ATTRIBUTES);
        item2.setItemMeta(meta);
        wmdpick = item2;

        ShapedRecipe sr = new ShapedRecipe(NamespacedKey.minecraft("wmd"), item2);
        sr.shape("FHF", "HpH", "FHF");
        sr.setIngredient('F', Material.NETHERITE_BLOCK);
        sr.setIngredient('p', Material.NETHERITE_PICKAXE);
        sr.setIngredient('H', Material.HEART_OF_THE_SEA);
        Bukkit.getServer().addRecipe(sr);
    }

